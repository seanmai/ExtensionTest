{
    "manifest_version": 2,
    "name": "Test Extension",
    "version": "0.1",

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"

            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "test_icon.png"
    }
}

/* Can also use * wildcards for URL */
/* ie) *://*google* */
